<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CommonImageTextView">
        <attr name="commonITViewSign" format="reference"/>
        <attr name="commonITViewSignH" format="reference"/>
        <attr name="commonITViewSignW" format="reference"/>
        <attr name="commonITViewSignBg" format="reference"/>
        <attr name="commonITViewTxt" format="reference|string"/>
        <attr name="commonITViewPadding" format="integer"/>
        <attr name="commonITViewTxtColor" format="color"/>
        <attr name="commonITViewDirection" format="enum">
            <enum name="left" value="0"/>
            <enum name="top" value="1"/>
            <enum name="right" value="2"/>
            <enum name="bottom" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="DesignsLinearLayoutExtend">
        <attr name="designsLLExtendMaxHeight" format="reference|integer"/>
    </declare-styleable>

    <declare-styleable name="DesignsRecycleViewExtend">
        <attr name="designsRVExtendMaxHeight" format="reference|integer"/>
    </declare-styleable>

    <declare-styleable name="DesignsSingleSet">
        <attr name="designsSSetTxt" format="reference|string"/>
        <attr name="designsSSetTxtColor" format="reference|color"/>
        <attr name="designsSSetSecondaryTxt" format="reference|string"/>
        <attr name="designsSSetSecondaryTxtColor" format="reference|color"/>
        <attr name="designsSSetRightStyle" format="enum">
            <enum name="img" value="0"/>
            <enum name="onOff" value="1"/>
            <enum name="txt" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="DesignsSwitchingFigureView">
        <attr name="designsDSFSparseArrayMethod" format="reference|string"/>
        <attr name="designsDSFSparseArrayMethodClassName" format="reference|string"/>
    </declare-styleable>


    <!--自定义单行菜单属性-->
    <declare-styleable name="DesignsCustomMenu">
        <attr name="designs_isLeft" format="boolean"/>
        <attr name="designs_leftID" format="reference"/>
        <attr name="designs_menuTxt" format="string"/>
        <attr name="designs_menuTxtSize" format="integer|reference"/>
        <attr name="designs_menuTxtColor" format="color|reference"/>
        <attr name="designs_secondaryTxt" format="string"/>
        <attr name="designs_isRight" format="boolean"/>
        <attr name="designs_rightID" format="reference"/>
        <attr name="designs_isWire" format="boolean"/>
        <attr name="designs_isSwitch" format="boolean"/>
        <attr name="designs_switchStyle" format="reference"/>
    </declare-styleable>

    <!--item侧滑删除属性-->

    <declare-styleable name="DesignsItemMenuLayout">
        <attr name="designsSwipeEnable" format="boolean"/>
        <attr name="designsIos" format="boolean"/>
        <attr name="designsLeftSwipe" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="DesignsSeekArc">
        <attr name="designs_thumb" format="reference" />
        <attr name="designs_thumbOffset" format="dimension" />
        <attr name="designs_max" format="integer" />
        <attr name="designs_progressWidth" format="dimension" />
        <attr name="designs_arcWidth" format="dimension" />
        <attr name="designs_progressMy" format="integer" />
        <attr name="designs_rotationMy" format="integer" />
        <attr name="designs_startAngle" format="integer" />
        <attr name="designs_sweepAngle" format="integer" />
        <attr name="designs_arcColor" format="color" />
        <attr name="designs_progressColor" format="color" />
        <attr name="designs_roundEdges" format="boolean" />
        <attr name="designs_touchInside" format="boolean" />
        <attr name="designs_clockwise" format="boolean" />
        <attr name="designs_enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DesignsSeekArcTheme">
        <attr name="designs_seekArcStyle" format="reference"/>
    </declare-styleable>
</resources>